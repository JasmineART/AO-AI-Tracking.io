{
  "name": "ao-ai-tracking.io",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://JasmineART.github.io/AO-AI-Tracking.io",
  "scripts": {
    "dev": "npm run serve-api & PORT=3001 npx webpack serve --mode development --port 3001 --no-open",
    "compile": "npm run build",
    "publish": "npm run deploy",
  "start:all": "concurrently \"npm:serve-api\" \"cross-env PORT=3001 webpack serve --mode development --port 3001 --no-open\"",
    "serve-api": "node server/dev-theme-api.js",
    "theme:dark": "node -e \"const fs=require('fs');fs.writeFileSync('dev-theme.json', JSON.stringify({theme:'dark'}, null, 2));console.log('dev-theme.json set to dark')\"",
    "theme:light": "node -e \"const fs=require('fs');fs.writeFileSync('dev-theme.json', JSON.stringify({theme:'light'}, null, 2));console.log('dev-theme.json set to light')\"",
    "start": "webpack serve --mode development --open",
    "test": "jest --runInBand --detectOpenHandles",
    "build": "webpack --mode production",
    "build:fast": "webpack --mode production --no-stats",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy:firebase": "npm run build && firebase deploy --only hosting --force",
    "deploy:rules": "firebase deploy --only database,firestore:rules --force",
    "deploy:all": "npm run build && firebase deploy --force",
    "deploy:quick": "./quick-deploy.sh",
    "clean": "rm -rf dist node_modules/.cache",
    "status": "firebase projects:list && git status",
    "bug-check": "./bug-check.sh",
    "health-check": "echo 'Run health check at http://localhost:3000/system-status'"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.1.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.6.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "firebase": "^12.4.0",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^29.6.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.14",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "chart.js": "^4.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "react-chartjs-2": "^5.3.0",
    "react-router-dom": "^7.9.4",
    "uuid": "^13.0.0"
  }
}
